# settings.env.example
# 실제 서비스 운영 시 이 파일을 복사해 settings.env(.env)로 사용하고, 각 변수에 실제 값을 입력하세요.

DB_HOST=             # 데이터베이스 호스트명(예: db, localhost)
DB_PORT=5432         # 데이터베이스 포트(기본: 5432)
DB_USER=             # 데이터베이스 사용자명(예: postgres)
DB_PASSWORD=         # 데이터베이스 비밀번호
DB_NAME=             # 데이터베이스 이름

TELEGRAM_BOT_TOKEN=  # 텔레그램 봇 토큰(실제 토큰 입력)
TELEGRAM_ADMIN_ID=   # 텔레그램 관리자(숫자 ID) 

DART_API_KEY=        # DART OpenAPI 인증키(https://opendart.fss.or.kr/) 